//all prime nums between 1 nd n
#include <stdio.h>

int main() {
  int n;
  printf("Please enter a number \n");
  scanf("%d", &n);

  for (int i = 2; i <= n; i++)
  {
    int iprime = 1;

    for (int j = 2; j < i; j++) {
      if (i % j == 0) {
        iprime = 0;
        break;
      }
    }

    if (iprime == 1) {
      printf("%d \n", i);
    }
  }

  return 0;
}
