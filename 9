//armstrong number

#include <math.h>
#include <stdio.h>

int main()
{
   int num, ogNum, remainder, n = 0;
   float result = 0.0;

   printf("Enter an integer: ");
   scanf("%d", &num);

   ogNum = num;

   for (ogNum = num; ogNum != 0; ++n) {
       ogNum /= 10;
   }

   for (ogNum = num; ogNum != 0; ogNum /= 10) {
       remainder = ogNum % 10;

      
      result += pow(remainder, n);
   }

 
   if ((int)result == num)
    printf("%d is an Armstrong number.", num);
   else
    printf("%d is not an Armstrong number.", num);
   return 0;
}
